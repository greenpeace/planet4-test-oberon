{
	"name": "greenpeace/planet4-test-oberon",
	"description": "Greenpeace P4 test-oberon",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-master-theme": "7",
		"greenpeace/planet4-plugin-gutenberg-blocks": "dev-main"
	},
	"scripts": {
		"core:language": "wp language core install pt_BR; wp language core activate pt_BR",
		"site:custom": [
			"@core:language"
		]
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "greenpeace/planet4-master-theme",
				"type": "wordpress-theme",
				"autoload": {
					"psr-4": {
						"P4\\MasterTheme\\": "src/"
					}
				},
				"extra": null,
				"version": 7,
				"dist": {
					"type": "zip",
					"url": "https://storage.googleapis.com/planet4-packages/planet4-master-theme/16d144a709140d8b85742b7cb2323605ac80ae5a.zip"
				}
			}
		}
	]
}
